{
  "consoleLog": {
    "prefix": "log",
    "body": ["console.log(${1:variable})"],
    "description": "Log to the console"
  },
  "detailedLog": {
    "prefix": "dlog",
    "body": ["console.log('${1:variable}', ${1:variable})"],
    "description": "Log to the console with a label"
  },
  "testBlock": {
    "prefix": "test",
    "body": ["test('${1:name}', () => { ${2:contents} })"],
    "description": "Testing `test` block"
  },
  "functionCallback": {
    "prefix": "fnc",
    "body": ["() => { ${1:contents} }"],
    "description": "Callback function"
  },
  "functionDeclaration": {
    "prefix": "fnd",
    "body": ["function ${1:name}(${2}) {", "  ${3:contents}", "}"],
    "description": "Function declaration"
  },
  "functionExpression": {
    "prefix": "fne",
    "body": ["const ${1:name} = (${2}) => {", "  ${3:contents}", "}"],
    "description": "Function expression"
  }
}
